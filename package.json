{
  "name": "kodnest-careers",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "db:generate": "pnpm --filter @kodnest/db exec prisma generate",
    "db:migrate": "pnpm --filter @kodnest/db exec prisma migrate dev",
    "db:studio": "pnpm --filter @kodnest/db exec prisma studio"
  },
  "devDependencies": {
    "turbo": "^2.0.0"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=20"
  }
}
